    <script type="text/javascript">

        if (window.location.hostname.toLowerCase().indexOf("alaraby.co.uk") > -1) {
            if (window.location.href.toLowerCase().indexOf("/diffah/") > -1) {

                document.getElementById('DiffhmainWarpper').style.display = "block";
            } else if (window.location.href.toLowerCase().indexOf("/jeel/") > -1) {

                document.getElementById('JeelmainWarpper').style.display = "block";
            } else if (window.location.href.toLowerCase().indexOf("/english/") > -1) {

                document.getElementById('EnMainWarpper').style.display = "block";
            } else {
                document.getElementById('mainWarpper').style.display = "block";
            }
        }
        else if (window.location.hostname.toLowerCase().indexOf("sehatok.com") > -1) {

            document.getElementById('SehatokmainWarpper').style.display = "block";

        }


        $(document).ready(function () {
            PrevInject();
            $(".search_Input_Container").find("button").bind("click", function () {
                if ($.trim($(this).parent().find("input").val()) != "") {

                    if ($(this).parent().find("input").hasClass("jeelSite")) {
                        window.location.href = "https://www.alaraby.co.uk/jeel/search?searchText=" + $(this).parent().find("input").val()
                    }
                    if ($(this).parent().find("input").hasClass("diffahSite")) {
                        window.location.href = "https://www.alaraby.co.uk/diffah/search?searchText=" + $(this).parent().find("input").val()
                    }
                    if ($(this).parent().find("input").hasClass("sehatokSite")) {
                        window.location.href = "https://www.sehatok.com/search?searchedtext=" + $(this).parent().find("input").val() + "&pagenumber=1"
                    }
                    if ($(this).parent().find("input").hasClass("arabiSite")) {
                        window.location.href = "https://www.alaraby.co.uk/search?searchedtext=" + $(this).parent().find("input").val() + "&pagenumber=1"
                    }
                    if ($(this).parent().find("input").hasClass("EnSite")) {
                        window.location.href = "https://www.alaraby.co.uk/english/search?searchedtext=" + $(this).parent().find("input").val() + "&pagenumber=1"
                    }
                }
                else {
                    if ($(this).parent().find("input").hasClass("EnSite")) {
                        $(this).parent().append("<div class='errValidate' >Please Enter a valid search word</div>")
                    }
                    else {
                        $(this).parent().append("<div class='errValidate' >يرجى إدخال كلمة البحث</div>")
                    }
                    $(this).parent().find(".errValidate").bind("click", function () {
                        $(this).fadeOut(function () {
                            $(this).remove();
                        })
                    })
                    var obj = $(this)
                    setTimeout(function () {
                        obj.parent().find(".errValidate").fadeOut(function () { obj.parent().find(".errValidate").remove(); })
                    }, 2000);
                }

            });
            activate_enter();
            //fixZoom()
        });
        function PrevInject() {
            $("input").keyup(function (e) {
                if (e.keyCode == 65 || e.keyCode == 97 || e.keyCode == 17) { // 'A' or 'a'
                    console.log("Control pressed");
                    e.preventDefault();
                    return false;
                } else {
                    if ($(this).attr("type") == "email")
                    { $(this).val($(this).val().replace(/[^أإا-يئءؤA-Z0-9-.@  ]+/i, '')) }
                    else {
                        $(this).val($(this).val().replace(/[^أإا-يئءؤA-Z0-9-.  ]+/i, ''))
                    }
                }
            });
        }

        function activate_enter() {
            $(".search_Input_Container>input[type=text]").keypress(function (e) {
                if (e.which == 13) {
                    $(this).next("button").trigger("click");
                }
            });
        }


    </script>

